    "# Tensor2 - (2X1) tensor\n",
    "tensor2 = nd.array((3,3,3)).reshape((3,1))\n",
    "\n",
    "# Broadcast Tensor2 onto Tensor1 over axis 1 \n",
    "add_result = nd.broadcast_add(lhs=tensor1, rhs=tensor2)\n",
    "print(add_result)"
   ]
